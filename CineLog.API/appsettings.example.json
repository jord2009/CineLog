{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=CineLogDb;Username=your_username;Password=your_password"
  },
  "TMDb": {
    "ApiKey": "your-tmdb-api-key-here"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-key-that-is-at-least-32-characters-long",
    "Issuer": "CineLog",
    "Audience": "CineLogUsers",
    "ExpirationInMinutes": 60
  },
  "AllowedHosts": "*"
}